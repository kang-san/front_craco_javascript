{
    "root": true,
    "plugins": [
        "react",
        "react-hooks",
        "jsx-a11y",
        "prettier"
    ],
    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true
    },
    "extends": [
        "airbnb",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": "error"
    }

}
